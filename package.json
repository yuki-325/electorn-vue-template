{
  "name": "my-electron-vue-app",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "webpack serve --config webpack.config.js",
    "electron": "electron .",
    "build": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.example.myapp",
    "productName": "MyElectronApp",
    "files": [
      "dist/**/*",
      "main.js"
    ],
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "buefy": "npm:@ntohq/buefy-next@^0.1.3",
    "css-loader": "^7.1.2",
    "vue": "^3.4.31",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.4.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.16"
  },
  "devDependencies": {
    "electron": "^31.1.0",
    "electron-builder": "^24.13.3",
    "html-webpack-plugin": "^5.6.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
